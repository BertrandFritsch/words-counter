{
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx",   
    "./typings/*"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es6",
      "dom"
    ],
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "allowJs": false
  }
}
